<html>
{{#each foo}}
{{this}}
{{/each}}
<h3> SONGS:</h3>
<form id="addsong" action="/song" method="post">
    Song name: <input type="text" name="song_name"><br>
    Artist: <input type="text" name="artist"><br>
    Album: <input type="text" name="album"><br>
    <input type="submit" value="Submit">
 </form>
<br/>

<h3>Current Songs:</h3>
<table>
    <thead>
        <th>Name</th>
        <th>Artist</th>
        <th>Album</th>
    </thead>
    <tbody>
        {{#each song}}
        <tr>
            <td>{{song_name}}</td>
            <td>{{artist}}</td>
            <td>{{album}}</td>
            <td><button onclick="deleteartist({{id}})">Delete</button></td>
            <td><a href="/song/{{id}}">Update</a></td>
        </tr>
        {{/each}}
    </tbody>
</table>
</html>
